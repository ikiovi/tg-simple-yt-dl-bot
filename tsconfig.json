{
  "compilerOptions": {
    "incremental": true,
    "lib": ["es2023"],
    "module": "NodeNext",
    "target": "es2022",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "outDir": "out",
    "noImplicitAny": true
  },
  "include": [ "src/" ],
}